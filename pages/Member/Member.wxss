/* Member.wxss */
page{
  height: 100%;
}
.body{
  height: 100%;
}
.m-top-user{
  font-size:13px;
  line-height: 1.5;
  padding:20rpx;
  background-color: #E51C23;
  color: #ffffff;
}
.m-top-user-img{
  width: 200rpx;
  text-align: center;
  margin: 0px auto;
}
.m-top-user-img image{
  width: 80px;
  height: 80px
}
.m-top-logout{
  -webkit-border-radius: 0;
  -moz-border-radius: 0;
  border-radius: 0;
  background-color: #ffffff;
}
button[type="default"] {
  color:#000000;
  background-color:#ffffff;
}

.m-top-bar{
  font-size: 13px;
  text-align: center;
  background-color: #ffffff;
}
.m-top-b{
  margin:5px;
  padding: 5px;
}
.m-top-bar text{
  display: block;
}
.member-fot{
  margin:20rpx 0; 
  background-color:#ffffff;
}
.m-fot{
  flex-wrap: wrap;
  -webkit-flex-wrap:wrap;
  justify-content: space-around;
  -webkit-justify-content: space-around;
}
.m-fot__item{
  width:33.333%;
  padding:30rpx 10rpx;
  font-size:16px;
  font-weight:bold;
  text-align:center;
  box-sizing:border-box;
  -moz-box-sizing:border-box;
  -webkit-box-sizing:border-box;
}
.m-fot__item image{
  width: 120rpx;
  height: 120rpx;
}
.m-fot__item text{
  display: block;
}
.share-but{
  position:absolute;
  top:0;
  left:0;
  padding:0;
  width:100%;
  height:100%;
  opacity:0;

}