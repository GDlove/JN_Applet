<!--applicationWithdrawal.wxml-->
<view class="body">
  <form bindsubmit="formSubmit">
    <view class="am-update weui-flex">
      <text>卡主姓名：</text>
      <input name="username" />
    </view>
    <view class="am-update weui-flex">
      <text>收款卡号：</text>
      <input name="cardnumber" type="number"/>
    </view>
    <view class="am-update weui-flex">
      <text>开户行：</text>
      <input name="bank" />
    </view>
    <view class="am-update weui-flex">
      <text>开户地址：</text>
      <input name="bankaddress" />
    </view>
    <view class="am-update weui-flex">
      <text>提现金额：</text>
      <input name="withdrawal" />
    </view>
    <view class="am-btn">
      <button type="default" formType="submit">提现</button>
    </view>
  </form>
</view>
