/* business.wxss */
.business-item{
  background-color: #ffffff;
  position: relative;
  height: 250rpx;
  margin:5px;
}
.business-orso{
  margin: 5px;
}
.business-item-img{
  width: 100%;
  height: 100%
}
.business-item-title{
  font-size: 18px;
  max-width: 300rpx;
}
.business-item-label text{
  background-color: #1AAC19;
  font-size: 12px;
  padding:5px 10px;  
  margin-right:20rpx;
  color: #333333
}
.business-item-btn{
  line-height:2;
  font-size: 15px;
  padding:0px;
  margin: 0px; 
  background-color: #148A96;
  color: #ffffff;
}